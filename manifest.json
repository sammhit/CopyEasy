{
  "manifest_version":2,


  "name":"CopyEasy",
  "description":"This Extension helps copy easy to your preferred DOcument editor",
  "version":"2.0",

  "browser_action":{
    "default_icon":"boy_off.png"   
  },
  "background":{
	"scripts":["background.js"]
	
  },
  "permissions":[
    "storage",
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*",
    "clipboardRead",
    "clipboardWrite"
   ],
  "web_accessible_resources":["testScript.js"] 
  
}
 
    

